{
    "name": "Replace Images",
    "version": "1.0",
    "description": "Replaces images idk wut you expect",
    "manifest_version": 2,
    "browser_action":{
        "default_title": "Replace Images",
        "default_popup": "popup.html"
    },"content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs"]
}